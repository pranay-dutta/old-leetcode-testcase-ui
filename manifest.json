{
  "manifest_version": 3,
  "name": "Old LeetCode Testcase UI",
  "version": "1.0",
  "description": "A Chrome extension to bring back the old LeetCode testcase UI.",
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "36": "icons/icon36.png",
      "48": "icons/icon48.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "36": "icons/icon36.png",
    "48": "icons/icon48.png"
  }
}
